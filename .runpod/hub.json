{
  "title": "Olmocr Endpoint",
  "description": "OCR endpoint using Qwen2VL model for PDF text extraction with anchor text support",
  "type": "serverless",
  "category": "image",
  "iconUrl": "https://example.com/icon.png",
  "config": {
    "runsOn": "GPU",
    "containerDiskInGb": 20,
    "presets": [
      {
        "name": "Standard OCR",
        "defaults": {
          "MAX_TOKENS": "512",
          "TEMPERATURE": "0.8",
          "TARGET_LENGTH": "4000"
        }
      }
    ],
    "env": [
      {
        "key": "MAX_TOKENS",
        "value": "512"
      },
      {
        "key": "TEMPERATURE",
        "value": "0.8"
      },
      {
        "key": "TARGET_LENGTH",
        "input": {
          "name": "Target Length",
          "type": "string",
          "description": "Target length for anchor text extraction",
          "default": "4000"
        }
      },
      {
        "key": "PDF_ENGINE",
        "input": {
          "name": "PDF Engine",
          "type": "string",
          "description": "PDF processing engine to use",
          "default": "pdfreport"
        }
      }
    ]
  }
}
